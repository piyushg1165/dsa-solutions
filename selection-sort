void selectionSort(vector<int>&arr) {
    // Write your code here.
    int n = arr.size() - 1;
 
    for(int i=0;i<n;i++)
{
    for(int j=i+1;j<=n;j++)
    {
        if(arr[j] < arr[i])
        {
            swap(arr[i] , arr[j]);
        }
    }
}
}
